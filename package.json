{
  "name": "pokemon-availability-checker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:1": "vite",
    "dev:2": "start http://localhost:5173",
    "build": "run-s build:*",
    "build:1": "node scripts/generateData.mjs",
    "build:2": "tsc && vite build",
    "lint": "run-s lint:*",
    "lint:1-eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:2-typescript": "tsc --project tsconfig.json --noEmit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mantine/core": "^7.8.1",
    "@mantine/hooks": "^7.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@nabla/vite-plugin-eslint": "^2.0.4",
    "@stylistic/eslint-plugin": "^1.7.2",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "^1.14.4",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}